{
  "name": "training-admin",
  "version": "1.0.0",
  "description": "This is a admin page",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix --ext .js .",
    "dev": "nodemon --exec babel-node ./src/bin/www.js",
    "start": "npm run build && node ./dist/bin/www.js",
    "clean": "rm -rf dist && mkdir dist",
    "build:babel": "babel ./src --out-dir dist --copy-files",
    "build": "yarn run clean && yarn run build:babel",
    "create:migration": "yarn knex migrate:make",
    "migrate:latest": "yarn knex migrate:latest",
    "migrate:rollback": "yarn knex migrate:rollback",
    "migrate:reset": "yarn run migrate:rollback && yarn run migrate:latest && yarn run seed:run",
    "seed:run": "yarn knex seed:run",
    "heroku-postbuild": "yarn install --production=false && yarn migrate:reset && yarn run build && yarn install --production",
    "knex": "babel-node node_modules/knex/bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangphu2412/admin-training.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "admin"
  ],
  "author": "dangphu2412",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dangphu2412/admin-training/issues"
  },
  "homepage": "https://github.com/dangphu2412/admin-training#readme",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@hapi/joi": "^17.1.1",
    "admin": "^1.4.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "http-status": "^1.5.0",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.19",
    "method-override": "^3.0.0",
    "mysql": "^2.18.1",
    "objection": "^2.2.15",
    "pg": "^8.5.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.9"
  }
}
